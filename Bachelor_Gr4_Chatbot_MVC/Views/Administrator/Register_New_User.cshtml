
@{
    ViewData["Title"] = "Register New User";
}

<div id="wrapper">

    @await Html.PartialAsync("_Navbar_admin")

    <!-- Page Content -->
    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">

                    <h1 class="page-header">Registrer ny bruker</h1>

                    <div class="col-lg-6">
                        <form role="form">
                            <div class="form-group">
                                <label>Epost</label>
                                <input class="form-control">
                                <p class="help-block">Brukerens epost</p>
                            </div>
                            <div class="form-group">
                                <label>Navn</label>
                                <input class="form-control">
                                <p class="help-block">Brukerens navn.</p>
                            </div>
                            <button type="submit" class="btn btn-default">Registrer bruker</button>
                        </form>
                    </div>
                    <div class="col-lg-6">
                        <p>
                            <span class="glyphicon glyphicon-info-sign"> </span>
                            Når brukeren registreres sendes det en epost til bruker med et passord han/hun kan benytte ved første innlogging
                        </p>
                    </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->
</div>
<!-- /#wrapper -->
<!-- jQuery -->
<script src="../vendor/jquery/jquery.min.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

<!-- Metis Menu Plugin JavaScript -->
<script src="../vendor/metisMenu/metisMenu.min.js"></script>

<!-- Custom Theme JavaScript -->
<script src="../dist/js/sb-admin-2.js"></script>

