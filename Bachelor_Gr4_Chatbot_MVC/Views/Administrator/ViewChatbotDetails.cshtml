                        <dd>
                            @Html.DisplayFor(model => model.tokenUrlExtension)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.conversationUrlExtension)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.conversationUrlExtension)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.botAutorizeTokenScheme)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.botAutorizeTokenScheme)
                        </dd>
                    </dl>
                </div>
                <div>
                    @Html.ActionLink("Endre innstillinger", "EditChatbotDetails", new { id = Model.chatbotId }) |
                    <a asp-action="Chatbots">Tilbake til listen</a>
