@model IEnumerable<Bachelor_Gr4_Chatbot_MVC.Models.AdministratorViewModel.ChatGroupViewModel>


<div id="wrapper">

    @await Html.PartialAsync("_Navbar_admin")


    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">


                    <!-- Page Content -->
                    <h4>Chat grupper</h4>
                    <p>
                        <a asp-action="RegisterChatGroup">Opprett ny chat gruppe</a>
                    </p>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.ChatGroupName)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ChatGroupName)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Edit", "EditChatGroup", new { id = item.ChatGroupId }) |
                                        @Html.ActionLink("Delete", "DeleteChatGroup", new { id = item.ChatGroupId })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>


                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->


</div>
<!-- /#wrapper -->



